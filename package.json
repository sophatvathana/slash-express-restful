    "cors": "^2.8.3",
    "cross-env": "^5.0.1",
    "d3-shape": "^1.2.0",
    "dotenv-safe": "^6.0.0",
    "draftjs-to-html": "^0.8.4",
    "draftjs-to-markdown": "^0.5.1",
    "dva-model-extend": "^0.1.2",
    "echarts": "^4.1.0",
    "echarts-for-react": "^2.0.11",
    "echarts-gl": "^1.1.1",
    "echarts-liquidfill": "^2.0.0",
    "express": "^4.15.2",
    "express-validation": "^1.0.2",
    "formidable": "^1.2.1",
