    "jwt-simple": "0.5.1",
    "less-vars-to-js": "^1.2.1",
    "lodash": "^4.17.10",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "log4js": "^2.10.0",
    "method-override": "^2.3.8",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.13",
    "mongoose": "^4.9.7",
    "morgan": "^1.8.1",
    "node-gyp": "^3.7.0",
    "nodemailer": "^4.6.7",
    "nprogress": "^0.2.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "4.0.0",
